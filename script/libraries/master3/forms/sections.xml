<?xml version="1.0" encoding="UTF-8"?>
<form>
    <fields name="form">
        <fieldset name="fsSection" label="TPL_MASTER3_SECTION_FORM_FIELDSET_SECTION">
            <field name="name" type="text" label="TPL_MASTER3_SECTION_FORM_NAME" class="input-medium input-large-text text-danger" readonly="true" />
            <field name="id" type="text" label="TPL_MASTER3_SECTION_FORM_ID" class="input-medium" />
            <field name="style" type="list" label="TPL_MASTER3_SECTION_FORM_STYLE" default="uk-section-default" class="input-medium">
                <option value="uk-section-default">TPL_MASTER3_SECTION_FORM_STYLE_DEFAULT</option>
                <option value="uk-section-muted">TPL_MASTER3_SECTION_FORM_STYLE_MUTED</option>
                <option value="uk-section-primary">TPL_MASTER3_SECTION_FORM_STYLE_PRIMARY</option>
                <option value="uk-section-secondary">TPL_MASTER3_SECTION_FORM_STYLE_SECONDARY</option>
            </field>
            <field name="light" type="radio" label="TPL_MASTER3_SECTION_FORM_LIGHT" default="0" class="btn-group">
                <option value="1">JYES</option>
                <option value="0">JNO</option>
            </field>
            <field name="padding" type="list" label="TPL_MASTER3_SECTION_FORM_PADDING" default="" class="input-medium">
                <option value="uk-section">TPL_MASTER3_SECTION_FORM_PADDING_DEFAULT</option>
                <option value="uk-section uk-section-xlarge">TPL_MASTER3_SECTION_FORM_PADDING_XLARGE</option>
                <option value="uk-section uk-section-large">TPL_MASTER3_SECTION_FORM_PADDING_LARGE</option>
                <option value="uk-section uk-section-small">TPL_MASTER3_SECTION_FORM_PADDING_SMALL</option>
                <option value="uk-section uk-section-xsmall">TPL_MASTER3_SECTION_FORM_PADDING_XSMALL</option>
                <option value="uk-section uk-padding-remove-vertical">TPL_MASTER3_SECTION_FORM_PADDING_NONE</option>
            </field>
            <field name="image" type="media" label="TPL_MASTER3_SECTION_FORM_IMAGE" class="input-medium" />
            <field name="class" type="text" label="TPL_MASTER3_SECTION_FORM_CLASS" />
        </fieldset>
        <fieldset name="fsGrid" label="TPL_MASTER3_SECTION_FORM_FIELDSET_GRID">
            <field name="container" type="list" label="TPL_MASTER3_SECTION_FORM_CONTAINER" default="" class="input-medium">
                <option value="uk-container">TPL_MASTER3_SECTION_FORM_CONTAINER_DEFAULT</option>
                <option value="uk-container uk-container-xsmall">TPL_MASTER3_SECTION_FORM_CONTAINER_XSMALL</option>
                <option value="uk-container uk-container-small">TPL_MASTER3_SECTION_FORM_CONTAINER_SMALL</option>
                <option value="uk-container uk-container-large">TPL_MASTER3_SECTION_FORM_CONTAINER_LARGE</option>
                <option value="uk-container uk-container-expand">TPL_MASTER3_SECTION_FORM_CONTAINER_EXPAND</option>
                <option value="uk-panel">TPL_MASTER3_SECTION_FORM_CONTAINER_NONE</option>
            </field>
            <field name ="gridspacer" type="spacer" hr="true" />
            <field name="responsive" type="list" label="TPL_MASTER3_SECTION_FORM_RESPOPNSIVE" default="medium" class="input-medium">
                <option value="large">TPL_MASTER3_SECTION_FORM_RESPOPNSIVE_LARGE</option>
                <option value="medium">TPL_MASTER3_SECTION_FORM_RESPOPNSIVE_MEDIUM</option>
                <option value="small">TPL_MASTER3_SECTION_FORM_RESPOPNSIVE_SMALL</option>
                <option value="stacked">TPL_MASTER3_SECTION_FORM_RESPOPNSIVE_STACKED</option>
            </field>
            <field name="gutter" type="list" label="TPL_MASTER3_SECTION_FORM_GUTTER" default="" class="input-medium">
                <option value="">TPL_MASTER3_SECTION_FORM_GUTTER_DEFAULT</option>
                <option value="uk-grid-small">TPL_MASTER3_SECTION_FORM_GUTTER_SMALL</option>
                <option value="uk-grid-medium">TPL_MASTER3_SECTION_FORM_GUTTER_MEDIUM</option>
                <option value="uk-grid-large">TPL_MASTER3_SECTION_FORM_GUTTER_LARGE</option>
                <option value="uk-grid-collapse">TPL_MASTER3_SECTION_FORM_GUTTER_COLLAPSE</option>
            </field>
            <field name="divider" type="radio" label="TPL_MASTER3_SECTION_FORM_DIVIDER" default="0" class="btn-group">
                <option value="1">JYES</option>
                <option value="0">JNO</option>
            </field>
            <field name="center" type="radio" label="TPL_MASTER3_SECTION_FORM_GRIDCENTER" default="0" class="btn-group">
                <option value="1">JYES</option>
                <option value="0">JNO</option>
            </field>
        </fieldset>
    </fields>
</form>
